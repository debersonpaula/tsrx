{
  "name": "tsrx",
  "description": "Typescript + React",
  "version": "1.0.0-beta.10",
  "main": "index.js",
  "scripts": {
    "build": "npm run clear && npm run build:source && npm run build:binary && npm run build:tools && node ./deploy/post-build.js",
    "build:source": "tsc -p deploy/tsconfig.source.json",
    "build:binary": "webpack --config ./deploy/webpack.config.deploy.js",
    "build:tools": "tsc -d -p deploy/tsconfig.tools.json",
    "clear": "rm -rf ./dist",
    "deploy": "npm run build && cd dist/bin && npm publish",
    "deploy:next": "npm run build && cd dist/bin && npm publish --tag next",
    "sb:start": "node dist/bin/tsrx start sandbox/config/react.config.js",
    "sb:build": "node dist/bin/tsrx build sandbox/config/react.config.js",
    "sb:test": "node dist/bin/tsrx test sandbox/config/react.config.js",
    "sb:lib": "node dist/bin/tsrx library sandbox/config/react.config.lib.js",
    "sb:static": "node dist/bin/tsrx build sandbox/config/react.config.static.js",
    "tsc": "tsc",
    "mfe-app1-start": "node dist/bin/tsrx start micro-frontend/app1/react.config.js",
    "mfe-app1-build": "node dist/bin/tsrx build micro-frontend/app1/react.config.js",
    "mfe-app2-start": "node dist/bin/tsrx start micro-frontend/app2/react.config.js",
    "mfe-app2-build": "node dist/bin/tsrx build micro-frontend/app2/react.config.js",
    "mfe-apps-build": "npm run mfe-app1-build && npm run mfe-app2-build",
    "mfe-main-start": "node dist/bin/tsrx start micro-frontend/main/react.config.js"
  },
  "bin": {
    "tsrx": "./index.js"
  },
  "author": "D.A.Paula <deberson.paula@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@hot-loader/react-dom": "17.0.1",
    "@svgr/webpack": "5.5.0",
    "@types/react": "17.0.13",
    "@types/react-dom": "17.0.8",
    "babel-loader": "8.2.2",
    "babel-plugin-named-asset-import": "0.3.7",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "5.2.6",
    "external-remotes-plugin": "1.0.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "process": "0.11.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "style-loader": "3.0.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.3.5",
    "url-loader": "4.1.1",
    "webpack": "5.43.0",
    "webpack-dev-server": "3.11.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "deepmerge": "4.2.2",
    "eslint": "7.30.0",
    "eslint-plugin-react": "7.24.0",
    "webpack-cli": "4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debersonpaula/tsrx.git"
  },
  "keywords": [
    "typescript",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/debersonpaula/tsrx/issues"
  },
  "homepage": "https://github.com/debersonpaula/tsrx#readme"
}
